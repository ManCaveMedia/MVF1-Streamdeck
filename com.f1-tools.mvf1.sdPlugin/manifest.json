{
	"SDKVersion": 2,
	"Author": "F1-Tools",
	"CodePath": "app.html",
	"Description": "Streamdeck plugin for controlling Multiviewer for Formula 1",
	"Name": "MVF1",
	"Icon": "actions/assets/action",
	"URL": "https://github.com/f1-tools/MVF1-Streamdeck",
	"Version": "1.0.0",
	"Software": {
		"MinimumVersion": "5.0"
	},
	"OS": [
		{
			"Platform": "mac",
			"MinimumVersion": "10.11"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		}
	],
	"Category": "MVF1",
	"CategoryIcon": "actions/assets/mv-rounded",
	"Actions": [
		{
			"Icon": "actions/assets/google_icons/sync.png",
			"Name": "Sync To Dialogue",
			"Controllers": ["Keypad"],
			"States": [
				{
					"Image": "actions/assets/google_icons/sync.png"
				}
			],
			"PropertyInspectorPath": "actions/property-inspector/default.html",
			"Tooltip": "Sync all MVF1 feeds to the current dialogue",
			"UUID": "com.f1-tools.mvf1.sync-to-dialogue"
		},
		{
			"Icon": "actions/assets/google_icons/play_pause.png",
			"Name": "Play/Pause All Feeds",
			"Controllers": ["Keypad"],
			"States": [
				{
					"Image": "actions/assets/google_icons/play_pause.png"
				}
			],
			"PropertyInspectorPath": "actions/property-inspector/default.html",
			"Tooltip": "Toggle Play/Pause of all MVF1 feeds",
			"UUID": "com.f1-tools.mvf1.play-pause-all"
		},
		{
			"Icon": "actions/assets/google_icons/replay.png",
			"Name": "Rewind All Feeds",
			"Controllers": ["Keypad"],
			"States": [
				{
					"Image": "actions/assets/google_icons/replay.png"
				}
			],
			"PropertyInspectorPath": "actions/property-inspector/rewind.html",
			"Tooltip": "Rewind all MVF1 feeds by x seconds",
			"UUID": "com.f1-tools.mvf1.rewind-all"
		},
		{
			"Icon": "actions/assets/google_icons/replay_mirrored.png",
			"Name": "Fast Forward All Feeds",
			"Controllers": ["Keypad"],
			"States": [
				{
					"Image": "actions/assets/google_icons/replay_mirrored.png"
				}
			],
			"PropertyInspectorPath": "actions/property-inspector/fast-forward.html",
			"Tooltip": "Fast Forward all MVF1 feeds by x seconds",
			"UUID": "com.f1-tools.mvf1.fast-forward-all"
		}
	]
}
